{
    "extends": [
      "tslint:recommended",
      "tslint-react",
      "tslint-react-hooks",
      "tslint-config-prettier"
    ],
    "rulesDirectory": [
      "tslint-plugin-prettier"
    ],
    "rules": {
      "prettier": true,
      "interface-name": false,
      "react-hooks-nesting": "error",
      "object-literal-sort-keys": false
    }
}